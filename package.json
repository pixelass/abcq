{
  "name": "abcq",
  "version": "0.0.1",
  "description": "",
  "repository": "git@github.com:pixelass/abcq.git",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "build": "npm run babel; node build",
    "create-index": "echo 'module.exports = require(\"./lib\").default;' > index.js",
    "docs": "rm -rf docs && mkdir docs && jsdoc -c jsdoc.json",
    "prebuild": "npm run docs",
    "precommit": "npm test",
    "prepublish": "npm run create-index; npm run build",
    "prepush": "npm test",
    "prerelease": "npm test",
    "pretest": "standard src",
    "release": "standard-version --no-verify"
  },
  "keywords": [],
  "author": "Gregor Adams <greg@pixelass.com> (http://pixelass.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "cz-conventional-changelog": "^1.2.0",
    "errorify": "^0.3.1",
    "hopsdoc": "^0.1.7",
    "husky": "^0.11.7",
    "jsdoc": "^3.4.1",
    "log": "^1.4.0",
    "standard": "^8.0.0",
    "standard-version": "^2.4.0",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0"
          ]
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
